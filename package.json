{
  "name": "pomello",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "pomello": "lib/index.js"
  },
  "scripts": {
    "watch": "concurrently --kill-others \"npm run watch:exec\" \"npm run watch:injected\"",
    "watch:exec": "babel exec --watch --out-dir lib",
    "watch:injected": "webpack --progress --colors --watch",
    "build": "rm -rf ./build && NODE_ENV=\"production\" && npm run build:exec && npm run build:injected",
    "build:exec": "babel exec/index.js --out-file lib/index.js",
    "build:injected": "webpack --progress --colors",
    "lint": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx || true"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "aphrodite": "^1.2.0",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "lodash.map": "^4.6.0",
    "mkdirp": "^0.5.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "nightmare": "^2.10.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "safe-write-file": "^1.1.2"
  },
  "description": "Generate react component fixtures and unit tests automagically",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cannoneyed/pomello.git"
  },
  "keywords": [
    "react",
    "testing",
    "tests",
    "test"
  ],
  "author": "cannoneyed",
  "bugs": {
    "url": "https://github.com/cannoneyed/pomello/issues"
  },
  "homepage": "https://github.com/cannoneyed/pomello#readme"
}
